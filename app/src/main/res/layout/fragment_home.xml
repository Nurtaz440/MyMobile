<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.main.user.home.HomeFragment"
        >

        <include
            android:id="@+id/partial_bottom_nav_controller"
            layout="@layout/bottom_nav_controller"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <TextView
            android:id="@+id/tv_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/uzmobile"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tv_main"
            >

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_item"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_margin="16dp"
                android:visibility="visible"
                app:cardBackgroundColor="@color/deep_sky_blue_100"
                app:cardCornerRadius="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="60dp"
                    app:srcCompat="@drawable/ic_uzmobile_logo"
                    tools:ignore="ContentDescription"
                    />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_call"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_call"
                        tools:ignore="ContentDescription"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="8dp"
                        android:text="@string/call_center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="23dp"
                        android:text="@string/call_center_number"
                        android:textColor="@color/black"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_balans"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="130dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_wallet"
                        tools:ignore="ContentDescription"

                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="18dp"
                        android:text="@string/balans"
                        android:textColor="@color/black"
                        android:textSize="21sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_item2"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_margin="16dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/alizarin_100"
                app:cardCornerRadius="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"

                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="60dp"
                    app:srcCompat="@drawable/ic_mobiuz_logo"
                    tools:ignore="ContentDescription"
                    />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_call_red"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_call"
                        app:tint="@color/alizarin_700"
                        tools:ignore="ContentDescription"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="8dp"
                        android:text="@string/call_center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="23dp"
                        android:text="@string/call_center_number"
                        android:textColor="@color/black"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_balans_red"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="130dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_wallet"
                        app:tint="@color/alizarin_700"
                        tools:ignore="ContentDescription"

                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="18dp"
                        android:text="@string/balans"
                        android:textColor="@color/black"
                        android:textSize="21sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_item3"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_margin="16dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/vivid_violet_100"
                app:cardCornerRadius="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"


                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="60dp"
                    app:srcCompat="@drawable/ic_ucell_logo"
                    tools:ignore="ContentDescription"
                    />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_call_violet"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_call"
                        app:tint="@color/vivid_violet_800"
                        tools:ignore="ContentDescription"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="8dp"
                        android:text="@string/call_center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="23dp"
                        android:text="@string/call_center_number"
                        android:textColor="@color/black"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_balans_violet"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="130dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_wallet"
                        app:tint="@color/vivid_violet_800"
                        tools:ignore="ContentDescription"

                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="18dp"
                        android:text="@string/balans"
                        android:textColor="@color/black"
                        android:textSize="21sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_item4"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:layout_margin="16dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/gorse_100"
                app:cardCornerRadius="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"


                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="60dp"
                    app:srcCompat="@drawable/ic_beeline_logo"
                    tools:ignore="ContentDescription"
                    />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_call_yellow"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="60dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_call"
                        app:tint="@color/gorse_600"
                        tools:ignore="ContentDescription"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="8dp"
                        android:text="@string/call_center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="23dp"
                        android:text="@string/call_center_number"
                        android:textColor="@color/black"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_balans_yellow"
                    android:layout_width="140dp"
                    android:layout_height="60dp"
                    android:layout_gravity="end"
                    android:layout_marginTop="130dp"
                    android:layout_marginEnd="24dp"
                    app:cardCornerRadius="16dp"
                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_margin="16dp"
                        app:srcCompat="@drawable/ic_wallet"
                        app:tint="@color/gorse_600"
                        tools:ignore="ContentDescription"

                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="18dp"
                        android:text="@string/balans"
                        android:textColor="@color/black"
                        android:textSize="21sp"
                        android:textStyle="bold"
                        />

                </androidx.cardview.widget.CardView>


            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cv_uzmobile"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="6dp"
            app:cardCornerRadius="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cl_main"
            app:layout_goneMarginStart="16dp"
            app:strokeColor="@color/deep_sky_blue_400"
            app:strokeWidth="1dp"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/ic_uzmobile_logo"
                tools:ignore="ContentDescription"
                />
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cv_mobiuz"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="7dp"
            android:layout_marginTop="6dp"
            app:cardCornerRadius="16dp"
            app:layout_constraintStart_toEndOf="@+id/cv_uzmobile"
            app:layout_constraintTop_toBottomOf="@+id/cl_main"
            app:layout_goneMarginStart="16dp"
            app:strokeColor="@color/alizarin_700"
            app:strokeWidth="1dp"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/ic_mobiuz_logo"
                tools:ignore="ContentDescription"
                />
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cv_ucell"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="7dp"
            android:layout_marginTop="6dp"
            app:cardCornerRadius="16dp"
            app:layout_constraintStart_toEndOf="@+id/cv_mobiuz"
            app:layout_constraintTop_toBottomOf="@+id/cl_main"
            app:layout_goneMarginStart="16dp"
            app:strokeColor="@color/vivid_violet_800"
            app:strokeWidth="1dp"
            >

            <ImageView
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/ic_ucell_logo"
                tools:ignore="ContentDescription"
                />

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cv_beeline"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_marginStart="7dp"
            android:layout_marginTop="6dp"
            app:cardCornerRadius="16dp"
            app:strokeColor="@color/gorse_600"
            app:strokeWidth="1dp"
            app:layout_constraintStart_toEndOf="@+id/cv_ucell"
            app:layout_constraintTop_toBottomOf="@+id/cl_main"
            app:layout_goneMarginStart="16dp"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="12dp"
                app:srcCompat="@drawable/ic_beeline_logo"
                tools:ignore="ContentDescription"
                />

        </com.google.android.material.card.MaterialCardView>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="424dp"
            android:layout_marginEnd="16dp"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    app:cardCornerRadius="16dp"
                    app:cardElevation="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_rate"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/tariflar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon2"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="8dp"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintStart_toEndOf="@+id/cv_icon"
                    app:layout_constraintTop_toTopOf="parent"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_internet_pockets"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/internet_paketlar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon3"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="16dp"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_icon"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_call_add"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/daqiqalar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />


                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon4"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintStart_toEndOf="@+id/cv_icon3"
                    app:layout_constraintTop_toBottomOf="@+id/cv_icon2"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_message"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/sms_paketlar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />


                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon5"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="16dp"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_icon3"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >


                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_ussd_code"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/ussd_kpdlar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />


                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_icon6"
                    android:layout_width="185dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    app:cardCornerRadius="16dp"
                    app:layout_constraintStart_toEndOf="@+id/cv_icon5"
                    app:layout_constraintTop_toBottomOf="@+id/cv_icon4"
                    >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal|center_vertical"
                        android:orientation="vertical"
                        >

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            app:srcCompat="@drawable/ic_service"
                            tools:ignore="ContentDescription"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="@string/xizmatlar"
                            android:textColor="@color/black"
                            android:textSize="18sp"
                            />


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
